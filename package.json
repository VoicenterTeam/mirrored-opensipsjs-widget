{
  "name": "opensipsjs-widget",
  "version": "0.0.0",
  "private": true,
  "author": "Bohdan Konechnyi",
  "main": "dist/opensips-widget.es.js",
  "types": "dist/types/opensips-widget.d.ts",
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "dev": "vite",
    "dev:documentation": "npm run build && cd docs && npm install && npm run dev",
    "build": "run-p type-check build-only",
    "build:documentation": "npm run build && cd docs && npm install && npm run build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@twind/core": "^1.1.3",
    "@twind/preset-autoprefix": "^1.0.7",
    "@twind/preset-tailwind": "^1.1.4",
    "@voicenter-team/opensips-js": "^1.0.19",
    "@vueuse/core": "^10.2.1",
    "construct-style-sheets-polyfill": "^3.1.0",
    "lodash": "^4.17.21",
    "regenerator-runtime": "^0.13.11",
    "v-loading-directive": "^1.0.4",
    "vue": "^3.3.4",
    "vue3-popper": "^1.5.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/lodash": "^4.14.196",
    "@types/node": "18.8.0",
    "@vitejs/plugin-vue": "3.1.2",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "8.5.0",
    "eslint-plugin-vue": "9.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.54.9",
    "typescript": "4.8.4",
    "vite": "^3.2.7",
    "vite-plugin-static-copy": "^0.17.0",
    "vite-svg-loader": "^4.0.0",
    "vue-tsc": "^0.40.4"
  }
}
