{
  "name": "opensips-js-widget",
  "version": "0.2.15",
  "author": "Bohdan Konechnyi",
  "main": "widget/opensipsjs-widget.mjs",
  "types": "widget/opensipsjs-widget.d.ts",
  "engines": {
    "node": ">= 16",
    "npm": "please-use-yarn",
    "yarn": ">= 1.22.4"
  },
  "scripts": {
    "postpublish": "yarn upload-cdn",
    "upload-cdn": "yarn build-widget && node scripts/upload-cdn.js",
    "dev": "vite",
    "dev-widget": "vite --config vite-widget.config.ts",
    "build-widget": "vite build --config vite-widget.config.ts",
    "build-old": "vite build --config vite.config-old.ts",
    "dev:documentation": "yarn build-widget && cd docs && yarn && yarn dev",
    "docs-build": "yarn build && cd docs && yarn && yarn run build",
    "build": "vite build",
    "build:documentation": "npm run build && cd docs && npm install && npm run build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "files": [
    "widget"
  ],
  "dependencies": {
    "@voicenter-team/voicenter-ui-plus": "0.7.1",
    "@vueuse/core": "^10.9.0",
    "construct-style-sheets-polyfill": "^3.1.0",
    "lodash": "^4.17.21",
    "opensips-js": "^0.1.7",
    "opensips-js-screen-share": "^0.0.1",
    "opensips-js-screen-share-whiteboard": "^0.0.1",
    "opensips-js-vue": "^0.1.6",
    "opensips-js-whiteboard": "^0.0.1",
    "regenerator-runtime": "^0.13.11",
    "v-loading-directive": "^1.0.4",
    "vue": "^3.5.12",
    "vue-slider-component": "^3.2.24",
    "vue3-popper": "^1.5.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@twind/core": "^1.1.3",
    "@twind/preset-autoprefix": "^1.0.7",
    "@twind/preset-tailwind": "^1.1.4",
    "@types/lodash": "^4.14.196",
    "@types/node": "18.8.0",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "@vitejs/plugin-vue": "^5.1.4",
    "@voicenter-team/aws-uploader": "^1.0.6",
    "@voicenter-team/eslint-config-vue": "1.0.21",
    "@vue/eslint-config-standard": "8.0.1",
    "@vue/eslint-config-typescript": "12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "eslint": "8.56.0",
    "eslint-plugin-vue": "9.19.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "postcss-selector-replace": "^1.0.2",
    "sass": "^1.69.7",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.2.2",
    "vite": "^5.4.0",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-html-config": "^1.0.11",
    "vite-plugin-static-copy": "^2.3.2",
    "vite-svg-loader": "^4.0.0",
    "vue-tsc": "^2.0.6"
  }
}
